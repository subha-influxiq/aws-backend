function loadNewsTicker() {
    $("#newsticker-wrapper ul").slideUp(500, function() {
        $.get(
            "callback_newsticker.php",
            function(data) {
                if (data.length > 0) {
                    $("#newsticker-wrapper").html(data);

                    if ($('.news_ticker_items li').length > 1) {
                        $('.news_ticker_items').newsTicker({
                            max_rows: 1,
                            speed: 600,
                            direction: 'up',
                            duration: 4000,
                            autostart: 1,
                            pauseOnHover: 0
                        });
                    }
                }
            },
            'html'
        );
    });
}

function clearOrcidSession() {
	/** force a log out before starting the OAuth process */
	$.ajax({
